@import "/asset/lib/wxParse/wxParse.wxss";

.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  box-sizing: border-box;
  background: rgba(0,0,0,.05);
  height: 100%;
} 
page{
  height: 100%;
  width: 100%;
}
.ellipsis{
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.no-more{
  text-align: center;
  color: #666;
  font-size: 25rpx;
  margin-top: 25rpx;
}